# Four.Meme Migration Sniper Configuration

# Blockchain Configuration
BSC_RPC_URL=https://bsc-dataseed.binance.org/
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545/
PRIVATE_RPC_URL=https://api.ankr.com/v1/bsc/your_api_key
TRADING_WALLET_PRIVATE_KEY=your_trading_wallet_private_key_here
TRADING_WALLET_ADDRESS=your_trading_wallet_address_here

# Migration Detection Configuration
MIGRATION_THRESHOLD=18
PRE_MIGRATION_BUFFER=0.5
MARKET_CAP_CHECK_INTERVAL=1000
MIGRATION_CONFIRMATION_BLOCKS=3
MAX_TOKENS_TO_MONITOR=50
ENABLE_PREDICTIVE_ANALYSIS=true
MARKET_CAP_CALCULATION_METHOD=HYBRID

# PancakeSwap Integration Configuration
PANCAKESWAP_ROUTER=0x10ED43C718714eb63d5aA57B78B54704E256024E
PANCAKESWAP_FACTORY=0xcA143Ce0Fe65960E6Aa4D42C8d3cE161c2B6604f
WBNB_ADDRESS=0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c
MAX_SLIPPAGE=0.05
DEADLINE_BUFFER=300
GAS_PRICE_MULTIPLIER=1.2
MAX_GAS_PRICE=20
MIN_LIQUIDITY_THRESHOLD=1
ENABLE_PRICE_IMPACT_PROTECTION=true
MAX_PRICE_IMPACT=0.1

# Transition Logic Configuration
DEFAULT_STRATEGY=aggressive
MAX_CONCURRENT_TRADES=5
MAX_TOTAL_EXPOSURE=1
ENABLE_RISK_MANAGEMENT=true
ENABLE_PROFIT_TAKING=true
ENABLE_STOP_LOSSES=true
GAS_PRICE_BUFFER=1.2
SLIPPAGE_BUFFER=0.05

# Trading Strategies Configuration
# Aggressive Strategy
AGGRESSIVE_BUY_AMOUNT=0.1
AGGRESSIVE_SELL_THRESHOLD=50
AGGRESSIVE_STOP_LOSS=20
AGGRESSIVE_MAX_HOLD_TIME=3600000
AGGRESSIVE_ENABLE_PARTIAL_SELLS=true
AGGRESSIVE_PARTIAL_SELL_PERCENTAGES=25,50,75

# Conservative Strategy
CONSERVATIVE_BUY_AMOUNT=0.05
CONSERVATIVE_SELL_THRESHOLD=20
CONSERVATIVE_STOP_LOSS=10
CONSERVATIVE_MAX_HOLD_TIME=1800000
CONSERVATIVE_ENABLE_PARTIAL_SELLS=false
CONSERVATIVE_PARTIAL_SELL_PERCENTAGES=

# MEV Protection Configuration
MEV_PROTECTION_ENABLED=true
PRIVATE_MEMPOOL=true
ANTI_FRONTRUN_ENABLED=true
ANTI_SANDWICH_ENABLED=true
MEV_PROTECTION_DELAY=100
GAS_PRICE_BUFFER=1.1
PRIORITY_FEE_BUFFER=1.2

# Gas Optimization Configuration
GAS_OPTIMIZATION_ENABLED=true
DYNAMIC_GAS_PRICING=true
GAS_PRICE_HISTORY_SIZE=100
NETWORK_CONGESTION_THRESHOLD=0.6
VALIDATOR_TIPS_ENABLED=true
VALIDATOR_TIP_AMOUNT=0.001

# Wallet Management Configuration
WALLET_MANAGEMENT_ENABLED=true
MAX_WALLETS=10
WALLET_FUNDING_AMOUNT=0.1
AUTO_FUND_WALLETS=false
WALLET_MONITORING_INTERVAL=5000

# Risk Management Configuration
RISK_MANAGEMENT_ENABLED=true
MAX_DAILY_LOSS=0.5
MAX_POSITION_SIZE=0.2
MAX_CORRELATION_RISK=0.8
ENABLE_POSITION_SIZING=true
ENABLE_DRAWDOWN_PROTECTION=true
MAX_DRAWDOWN_PERCENTAGE=20

# Monitoring Configuration
MONITORING_INTERVAL=5000
MIGRATION_MONITORING_ENABLED=true
PERFORMANCE_TRACKING=true
POSITION_MONITORING=true
GAS_MONITORING=true

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/four-meme-migrate-sniper
REDIS_URL=redis://localhost:6379

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/four-meme-migrate-sniper.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5

# API Configuration
API_PORT=3001
API_HOST=localhost
CORS_ORIGIN=http://localhost:3000

# WebSocket Configuration
WS_PORT=3002
WS_HOST=localhost

# Notification Configuration
ENABLE_TELEGRAM_NOTIFICATIONS=false
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

ENABLE_DISCORD_NOTIFICATIONS=false
DISCORD_WEBHOOK_URL=your_discord_webhook_url

# Security Configuration
ENABLE_RATE_LIMITING=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Performance Configuration
ENABLE_CACHING=true
CACHE_TTL=300
MAX_CACHE_SIZE=1000

# Monitoring and Metrics
ENABLE_METRICS=true
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=30000

# Development Configuration
NODE_ENV=development
DEBUG=false
ENABLE_HOT_RELOAD=false
